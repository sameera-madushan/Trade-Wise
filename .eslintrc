{
	"root": true,
	"extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "@vue/eslint-config-prettier", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"env": {
		"node": true,
		"amd": true
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-console": "warn",
		"func-names": "off",
		"quotes": "off",
		"camelcase": "off",
		"no-undef": "off",
		"no-prototype-builtins": "off",
		"vue/no-unused-vars": "warn",
		"vue/script-setup-uses-vars": "off",
		"vue/no-useless-template-attributes": "off",
		"vue/multi-word-component-names": "off",
		"vue/no-reserved-component-names": "off"
	}
}
